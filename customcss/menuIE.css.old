
ul#cssmenu3, ul#cssmenu3 ul {
    list-style: none outside none;
    margin: 0;
    padding: 0;
}
ul#cssmenu3, ul#cssmenu3 .submenu {
    background-color: #1F1F1F;
    border-radius: 5px 5px 5px 5px;
    border-style: solid;
    border-width: 0;
}
ul#cssmenu3 .submenu {
    background-color: #2A2A2A;
    border-radius: 0 5px 5px 5px;
    float: left;
    left: 0;
    opacity: 0;
    padding: 0 3px 3px;
    position: absolute;
    top: 100%;
    transition: all 0.5s ease 0s;
    visibility: hidden;
}
ul#cssmenu3 li:hover > .submenu {
    opacity: 1;
    visibility: visible;
}
ul#cssmenu3 li {
    display: block;
    float: left;
    font-size: 0;
    position: relative;
    white-space: nowrap;
}
ul#cssmenu3 li:hover {
    z-index: 1;
}
ul#cssmenu3 ul .submenu {
    border-color: #434343;
    border-style: solid;
    border-width: 1px;
    left: 100%;
    position: absolute;
    top: 0;
}
ul#cssmenu3 > li:hover > .submenu {
    opacity: 0.8;
}
ul#cssmenu3 {
    display: inline-block;
    font-size: 0;
    padding: 6px 6px 6px 0;
    position: relative;
    z-index: 999;
}
ul#cssmenu3 .column {
    float: left;
}
* html ul#cssmenu3 li a {
    display: inline-block;
}
ul#cssmenu3 > li {
    margin: 0 0 0 6px;
}
ul#cssmenu3 ul > li {
    margin: 6px 0 0;
}
ul#cssmenu3 a:active, ul#cssmenu3 a:focus {
    outline-style: none;
}
ul#cssmenu3 a {
    background-color: transparent;
    background-repeat: repeat;
    border-style: none;
    border-width: 0;
    color: #C0C0BA;
    cursor: pointer;
    display: block;
    font: 12px Lucida Sans Unicode,sans-serif;
    padding: 7px 14px;
    text-align: left;
    text-decoration: none;
    vertical-align: middle;
}
ul#cssmenu3 ul li {
    float: none;
    margin: 3px 0 0;
}
ul#cssmenu3 ul a {
    background-color: #2A2A2A;
    border-radius: 5px 5px 5px 5px;
    color: #C0C0BA;
    font: 12px Lucida Sans Unicode,sans-serif;
    padding: 5px;
    text-align: left;
    text-decoration: none;
    transition: all 0.5s ease 0s;
}
ul#cssmenu3 li:hover > a, ul#cssmenu3 li a.pressed {
    background-color: #2A2A2A;
    border-style: none;
    color: #FFFFFF;
    text-decoration: none;
}
ul#cssmenu3 img {
    border: medium none;
    margin-right: 7px;
    vertical-align: middle;
}
ul#cssmenu3 span {
    background-position: right center;
    background-repeat: no-repeat;
    display: block;
    overflow: visible;
    padding-right: 0;
}
ul#cssmenu3 ul span {
   /* background-image: url("arrowsub.png");*/
    padding-right: 9px;
}
ul#cssmenu3 ul li:hover > a, ul#cssmenu3 ul li a.pressed {
    background-color: #525252;
    color: #FFFFFF;
    text-decoration: none;
}
ul#cssmenu3 li.topmenu > a {
    border-color: transparent;
    border-radius: 5px 5px 5px 5px;
    border-style: solid;
    border-width: 1px;
    opacity: 0.8;
    transition: all 0.5s ease 0s;
}
ul#cssmenu3 li.topmenu:hover > a, ul#cssmenu3 li.topmenu a.pressed {
    border-color: #434343;
    border-style: solid;
}
ul#cssmenu3 li.has-sub > a {
    border-color: transparent;
    border-radius: 5px 5px 0 0;
    border-style: solid;
    border-width: 1px 1px 0;
    opacity: 0.8;
    transition: all 0.5s ease 0s;
}
ul#cssmenu3 li.has-sub:hover > a, ul#cssmenu3 li.has-sub a.pressed {
    border-color: #434343;
    border-style: solid;
}

